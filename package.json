  "scripts": {
    "dev": "concurrently \"npm:watch:*\"",
    "watch:eleventy": "NODE_ENV=development eleventy --serve",
    "watch:tailwind": "NODE_ENV=development tailwindcss -i ./theme/css/tailwind.css -o ./_site/prin.css --watch",
    "debug": "DEBUG=Eleventy* eleventy --serve",
    "debug:performance": "DEBUG=Eleventy:Benchmark* eleventy --serve",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production tailwindcss -i ./theme/css/tailwind.css -o ./_site/prin.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Princesseuh/pricesseuh.dev.git"
  },

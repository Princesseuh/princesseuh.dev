    "eleventy-plugin-nesting-toc": "^1.3.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "quick-score": "^0.0.12",
    "quicklink": "^2.2.0",
    "shorthash2": "^1.0.3"
